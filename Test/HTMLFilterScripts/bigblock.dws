			<div id="orderdata">
				<table>
					<tbody style="text-align: right;">
						<tr>
							<td>Description</td>
							<td>
								<input
									id="orderdesc"
									type="text"
									style="width: 100%;"
									/>
							</td>
							<td rowspan="2" style="text-align: center; width: 100px;">
								<button onclick="addOrder();">Add</button>
							</td>
						</tr>
						<tr>
							<td>Order number</td>
							<td>
								<input
									id="orderno"
									type="text"
									style="width: 100%;"
									/>
							</td>
						</tr>
					</tbody>
				</table>
